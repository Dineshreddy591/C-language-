#include<stdio.h>
#include<conio.h>
 int main()
{
	int n,sum=0,rem,temp;
	printf("Enter a number:");
	scanf("%d",&n);
	temp = n;
	while(n>0)
	{
		rem=n%10;
		sum += rem * rem * rem;
		n=n/10;
	}
	if(temp == sum)
        printf("Amstrong numer :%d",sum);
    else
        printf("Not Amstrong :%d",sum);
	return 0;
}
